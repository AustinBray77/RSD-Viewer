{
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devPath": "http://localhost:1420",
        "distDir": "../dist",
        "withGlobalTauri": false
    },
    "package": {
        "productName": "RSD-Viewer",
        "version": "1.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "process": {
                "all": false,
                "exit": true
            },
            "dialog": {
                "all": false,
                "open": true
            },
            "shell": {
                "all": false,
                "open": true
            }
        },
        "bundle": {
            "active": true,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "resources": ["resources/*"],
            "identifier": "com.SBG.rsd-viewer",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": true,
            "endpoints": [
                "https://gist.githubusercontent.com/AustinBray77/ec1ceb7c036cbb28defa882487fcc00b/raw/cb9b22ee679536a73bfc7c0ad5839d7439434f0a/RSD-updater.json"
            ],
            "dialog": true,
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIzODA2QTA2QjhBRjZGQjgKUldTNGI2KzRCbXFBSTFPL1NTV0FaemNic2dyTmM4Ulh5S0x4dFhPV3VxdG1ZYnF5WkxDdEhLYy8K"
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "RSD-Viewer",
                "width": 1300,
                "height": 800
            }
        ]
    }
}
